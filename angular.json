{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ast": {
      "projectType": "library",
      "root": "projects/ast",
      "sourceRoot": "projects/ast/src",
      "prefix": "ast",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ast/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ast/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ast/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ast/src/test.ts",
            "tsConfig": "projects/ast/tsconfig.spec.json",
            "karmaConfig": "projects/ast/karma.conf.js"
          }
        }
      }
    },
